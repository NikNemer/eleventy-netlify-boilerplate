<head>
  <meta charset="utf-8">
  <link rel="dns-prefetch" href="https://identity.netlify.com">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>{{ renderData.title or title or metadata.title }}</title>
  <meta name="description" content="{{ summary or metadata.description }}">
  <meta property="og:site_name" content="{{ metadata.title }}">
  <meta property="og:title" content="{{ renderData.title or title or metadata.title }}">
  <meta property="twitter:title" content="{{ renderData.title or title or metadata.title }}">
  <meta property="og:description" content="{{ summary or metadata.description }}">
  <meta property="twitter:description" content="{{ summary or metadata.description }}">
  <link rel="canonical" href="{{ page.url }}">
  <meta property="og:url" content="{{ page.url }}">
  {% if mimage %}
  <meta property="og:type" content="article">
  <meta property="og:image" content="{{ metadata.url }}{{ mimage }}">
  <meta property="twitter:image" content="{{ metadata.url }}{{ mimage }}">
  <link rel="image_src" href="{{ metadata.url }}{{ mimage }}">
  {% endif %}
  <script async src="https://identity.netlify.com/v1/netlify-identity-widget.js"></script>
  {% set css %}
    {% include "assets/css/inline.css" %}
  {% endset %}
  <style>{{ css | cssmin | safe }}</style>
  {% set js %}
    {% include "assets/js/inline.js" %}
  {% endset %}
  <script>{{ js | jsmin | safe }}</script>
</head>
